{
  "name": "relude-eon",
  "version": "0.2.0",
  "description": "A datetime tool for the ages in ReasonML",
  "main": "index.js",
  "scripts": {
    "clean": "bsb -clean-world",
    "build": "bsb -make-world",
    "watch": "bsb -make-world -w",
    "test": "jest",
    "coverage": "jest --coverage --coverage-reporters=lcov --coverage-reporters=text --coverage-directory ./docs/coverage",
    "cleanbuild": "npm run clean && npm run build",
    "cleantest": "npm run cleanbuild && npm run test",
    "cleancoverage": "npm run cleanbuild && npm run coverage",
    "start": "npm run watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reazen/relude-eon.git"
  },
  "keywords": [
    "date",
    "datetime",
    "instant",
    "reason",
    "bucklescript"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reazen/relude-eon/issues"
  },
  "homepage": "https://github.com/reazen/relude-eon#readme",
  "dependencies": {},
  "peerDependencies": {
    "bs-bastet": "^1.2.5",
    "relude": "^0.59.0"
  },
  "devDependencies": {
    "@glennsl/bs-jest": "^0.5.1",
    "bs-bastet": "^1.2.5",
    "bs-platform": "^7.2.2",
    "relude": "^0.59.0"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "./test"
    ],
    "testMatch": [
      "**/test/*.js"
    ]
  }
}
